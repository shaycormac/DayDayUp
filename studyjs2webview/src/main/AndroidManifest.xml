<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studyjs2webview">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Js2JavaActivity" />
        <activity
            android:name=".activity.CardViewActivity"
            android:label="@string/title_activity_card_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.CoordinatorLayoutActivity"
            android:label="@string/title_activity_coordinator_layout"
            android:theme="@style/AppTheme.Immersive"></activity>
        <activity
            android:name=".activity.ViewTreeObserverActivity"
            android:label="@string/title_activity_view_tree_observer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.LoaderStudyActivity"
            android:label="@string/title_activity_loader_study"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.GestureStudyActivity" />
        <activity android:name=".activity.SharePreferenceActivity"></activity>
    </application>

</manifest>